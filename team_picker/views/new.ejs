<%- include('./partials/header') %>
    <div class="container">
        <form action="/cohorts" method="post">
            <div class="form-group">
            <label for="exampleFormControlInput1">Logo Url</label>
            <input type="text" name="logoUrl" class="form-control" id="exampleFormControlInput1" placeholder="e.g.https://unsplash.com/photos/G85VuTpw6jg">
            </div>

            <div class="form-group">
            <label for="exampleFormControlInput1">Name</label>
            <input type="text" name="name" class="form-control" id="exampleFormControlInput1" placeholder="e.g. Team North">
            </div>

            <div class="form-group">
            <label for="exampleFormControlTextarea1">Members</label>
            <textarea name="members"class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="e.g. Jon, Cersei, Ned, Arya, Daenerys"></textarea>
            </div> 

            <div class="d-flex justify-content-end">
            <input class="btn btn-primary  mt-3" type="submit">
            </div>
            </div>
        </form>
    </div>
<%- include('./partials/footer') %>